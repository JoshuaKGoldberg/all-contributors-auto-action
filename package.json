{
	"name": "all-contributors-auto-action",
	"version": "0.4.0",
	"description": "Fills in missing allcontributors entries for a repository. ðŸ‘ª",
	"repository": {
		"type": "git",
		"url": "https://github.com/JoshuaKGoldberg/all-contributors-auto-action"
	},
	"license": "MIT",
	"author": {
		"name": "JoshuaKGoldberg",
		"email": "npm@joshuakgoldberg.com"
	},
	"type": "module",
	"main": "./lib/index.js",
	"files": [
		"lib/",
		"package.json",
		"LICENSE.md",
		"README.md"
	],
	"scripts": {
		"build": "tsc",
		"build:release": "ncc build src/index.ts -o dist",
		"format": "prettier \"**/*\" --ignore-unknown",
		"format:write": "pnpm format --write",
		"lint": "eslint . .*js --max-warnings 0 --report-unused-disable-directives",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:package-json": "npmPkgJsonLint ./package.json",
		"lint:packages": "pnpm dedupe --check",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "husky install",
		"test": "vitest"
	},
	"lint-staged": {
		"*": [
			"prettier --ignore-unknown --write"
		]
	},
	"dependencies": {
		"@actions/core": "^1.10.0",
		"@actions/github": "^6.0.0",
		"@types/node": "^20.0.0",
		"all-contributors-for-repository": "^0.2.0"
	},
	"devDependencies": {
		"@types/eslint": "^8.44.2",
		"@typescript-eslint/eslint-plugin": "^6.5.0",
		"@typescript-eslint/parser": "^6.5.0",
		"@vercel/ncc": "^0.38.0",
		"@vitest/coverage-v8": "^1.0.0",
		"console-fail-test": "^0.2.3",
		"cspell": "^8.0.0",
		"eslint": "^8.48.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-deprecation": "^2.0.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jsdoc": "^48.0.0",
		"eslint-plugin-jsonc": "^2.9.0",
		"eslint-plugin-markdown": "^3.0.1",
		"eslint-plugin-n": "^16.0.2",
		"eslint-plugin-no-only-tests": "^3.1.0",
		"eslint-plugin-perfectionist": "^2.0.0",
		"eslint-plugin-regexp": "^2.0.0",
		"eslint-plugin-vitest": "^0.3.1",
		"eslint-plugin-yml": "^1.8.0",
		"husky": "^8.0.3",
		"jsonc-eslint-parser": "^2.3.0",
		"knip": "3.10.0",
		"lint-staged": "^15.0.0",
		"markdownlint": "^0.32.0",
		"markdownlint-cli": "^0.38.0",
		"npm-package-json-lint": "^7.0.0",
		"npm-package-json-lint-config-default": "^6.0.0",
		"prettier": "^3.0.3",
		"prettier-plugin-curly": "^0.1.2",
		"prettier-plugin-packagejson": "^2.4.5",
		"sentences-per-line": "^0.2.1",
		"typescript": "^5.2.2",
		"vitest": "^1.0.0",
		"yaml-eslint-parser": "^1.2.2"
	},
	"packageManager": "pnpm@8.13.1",
	"engines": {
		"node": ">=18"
	},
	"publishConfig": {
		"provenance": true
	}
}
