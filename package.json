{
	"name": "all-contributors-auto-action",
	"version": "0.5.0",
	"description": "Fills in missing allcontributors entries for a repository. ðŸ‘ª",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/JoshuaKGoldberg/all-contributors-auto-action.git"
	},
	"license": "MIT",
	"author": {
		"name": "JoshuaKGoldberg",
		"email": "npm@joshuakgoldberg.com"
	},
	"type": "module",
	"main": "lib/index.js",
	"files": [
		"lib/"
	],
	"scripts": {
		"build": "tsc",
		"build:release": "ncc build src/index.ts -o dist",
		"format": "prettier .",
		"format:write": "pnpm format --write",
		"lint": "eslint . --max-warnings 0",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:packages": "pnpm dedupe --check",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "husky",
		"test": "vitest",
		"tsc": "tsc"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"dependencies": {
		"@actions/core": "^3.0.0",
		"@actions/github": "^9.0.0",
		"@types/node": "^24.0.0",
		"all-contributors-for-repository": "^0.4.0"
	},
	"devDependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "4.6.0",
		"@eslint/js": "9.39.0",
		"@types/eslint-plugin-markdown": "2.0.2",
		"@types/node": "24.10.0",
		"@vercel/ncc": "0.38.4",
		"@vitest/coverage-v8": "4.0.1",
		"@vitest/eslint-plugin": "1.6.1",
		"console-fail-test": "0.6.0",
		"create-typescript-app": "2.46.3",
		"cspell": "9.6.0",
		"eslint": "9.39.0",
		"eslint-plugin-jsdoc": "62.5.0",
		"eslint-plugin-jsonc": "2.21.0",
		"eslint-plugin-markdown": "5.1.0",
		"eslint-plugin-n": "17.23.1",
		"eslint-plugin-package-json": "0.88.0",
		"eslint-plugin-perfectionist": "5.5.0",
		"eslint-plugin-regexp": "3.0.0",
		"eslint-plugin-yml": "3.1.0",
		"husky": "9.1.7",
		"knip": "5.83.0",
		"lint-staged": "16.2.3",
		"markdownlint": "0.40.0",
		"markdownlint-cli": "0.47.0",
		"prettier": "3.8.0",
		"prettier-plugin-curly": "0.4.0",
		"prettier-plugin-packagejson": "3.0.0",
		"prettier-plugin-sh": "0.18.0",
		"sentences-per-line": "0.3.0",
		"typescript": "5.9.3",
		"typescript-eslint": "8.54.0",
		"vitest": "4.0.1"
	},
	"packageManager": "pnpm@10.28.0",
	"engines": {
		"node": ">=18"
	}
}
